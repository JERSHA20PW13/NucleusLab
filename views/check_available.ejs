<%-include ('partials/header') %>
    <div class="schedule rounded p-3 mx-lg-5 ">
        <div class="m-3 row">
            <div class="mb-3  text-center">
                <h2 class="text-center float-sm-start">Check Availability</h2>
            </div>
        </div>
        <div class="check_available mb-3 d-flex justify-content-center ">
            <form action="/check_available" method="post" style="width: 30%;">


                <div class="mb-3 row">
                    <label for="date" class="col-xl-5 col-form-label">Date</label>
                    <div class="col-xl-7">
                        <input type="date" class="form-control" id="date" name="date" required>
                    </div>
                </div>

                <div class="mb-3 row">
                    <label for="from" class="col-xl-5 col-form-label">From period</label>
                    <div class="col-xl-7">
                        <select class="form-select" aria-label="Default select example" id="from" name="from" required>
                            <option></option>
                            <option>1</option>
                            <option>2</option>
                            <option>3</option>
                            <option>4</option>
                            <option>5</option>
                            <option>6</option>
                            <option>7</option>
                            <option>8</option>
                            <option>9</option>
                            <option>10</option>
                        </select>
                    </div>
                </div>

                <div class="mb-3 row">
                    <label for="to" class="col-xl-5 col-form-label">To period</label>
                    <div class="col-xl-7">
                        <select class="form-select" aria-label="Default select example" id="to" name="to" required>
                            <option></option>
                            <option>1</option>
                            <option>2</option>
                            <option>3</option>
                            <option>4</option>
                            <option>5</option>
                            <option>6</option>
                            <option>7</option>
                            <option>8</option>
                            <option>9</option>
                            <option>10</option>
                        </select>
                    </div>
                </div>

                <div class="text-center">
                    <input class="btn btn-block request" type="submit" name="submit" value="Check">
                </div>

            </form>
        </div>

        <% if(locals.table) { %>
        <div class="table-responsive">
            <table class="table table-striped table-hover table-bordered">
                <thead>
                    <tr>
                        <th scope="col">Lab</th>
                        <% for(var i=from; i<=to; i++) { %>
                            <th><%= i %></th>
                        <% } %>
                    </tr>
                </thead>
                <tbody>
                    <tr>
                        <th scope="row" class="rhead">CSL1</th>
                        <% table[0].forEach(entry=>{ %>
                        <% if(entry == "free") { %>
                            <td><strong class="text-purple"><%= entry %></strong></td>
                        <% }else{ %>
                            <td><%= entry %></td>
                        <% } }) %>
                    </tr>

                    <tr>
